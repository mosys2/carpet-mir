    <section class="inner-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-6">
                    <div class="details-gallery">
                        <div class="details-label-group">
                            <label class="details-label new">جدید</label>
                            @if(Model.Discount>0)
                            {
                                <label dir="ltr" class="details-label off">@Model.Discount%</label>
                            }
                        </div>
                        <ul class="details-preview">
                            @foreach (var image in Model.UrlImagList)
                            {
                            <li><img src="@image.Url" alt="product"></li>
                            }
                           
                        </ul>
                        <ul class="details-thumb">
                            @foreach (var image in Model.UrlImagList)
                            {
                                <li><img src="@image.Url" alt="product"></li>
                            }
                        </ul>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="details-content">
                        <h3 class="details-name"><a href="#">@Model.Name</a></h3>
                        <div class="details-meta">
                            <p>کد محصول:<span>@Model.CodeProduct</span></p>
                            <p>برند:<a href="#">@Model.Brand</a></p>
                        </div>
                        <div class="details-rating">
                            @for (int i = 1; i <= Model.Star; i++)
                            {
                                <i class="active icofont-star"></i>

                            }
                             @for (int i = 1; i <=(5-Model.Star); i++)
                            {
                             <i class="icofont-star"></i>

                            }
                          
                        </div>
                        <h3 class="details-price">
                            @if (Model.Price >= Model.LastPrice)
                            {
                                <span>@Model.Price @Model.Unit</span>
                            }
                            else
                            {
                                <span>@Model.Price @Model.Unit</span>
                                <del>@Model.LastPrice @Model.Unit</del>
                            }
                        </h3>
                        <p class="details-desc">@Model.Description</p>
                        <div class="details-list-group">
                            <label class="details-list-title">تگ ها:</label>
                            <ul class="details-tag-list">
                                @foreach (var item in Model.Tags)
                                {
                              <li><a href="#">@item.Name</a></li>

                                }
                               
                            </ul>
                        </div>
                        <div class="details-list-group">
                            <label class="details-list-title">اشتراک:</label>
                            <ul class="details-share-list">
                                <li><a href="#" class="icofont-facebook" title="Facebook"></a></li>
                                <li><a href="#" class="icofont-twitter" title="Twitter"></a></li>
                                <li><a href="#" class="icofont-linkedin" title="Linkedin"></a></li>
                                <li><a href="#" class="icofont-instagram" title="Instagram"></a></li>
                            </ul>
                        </div>
                        <div class="details-add-group">
                            <button class="product-add" onclick="AddToCart('@Model.Id')" title="افزودن به سبد خرید">
                                <i class="fas fa-shopping-basket"></i>
                                <span>افزودن به سبد خرید</span>
                            </button>
                            <div class="product-action">
                                <button class="action-minus" title="کم کردن"><i class="icofont-minus"></i></button>
                                <input class="action-input" title="تعداد مورد نیاز" type="text" name="quantity" value="1">
                                <button class="action-plus" title="زیاد کردن"><i class="icofont-plus"></i></button>
                            </div>
                        </div>
                        <div class="details-action-group">
                            <a class="view-wish wish" href="#" title="افزودن به لیست علاقه مندی ها">
                                <i class="icofont-heart"></i>
                                <span>افزودن به علاقه مندی</span>
                            </a>
