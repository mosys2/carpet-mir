@using Store.Application.Services.SiteContacts.Queries.GetSocialMediaForSite;
@model List<GetSocialMediaSiteDto>
@if (Model.Count > 0)
{
    <div class="social-icon-style-8 d-inline-block align-middle">
        <ul class="small-icon no-margin-bottom">
           @foreach (var social in Model)
            {
                <li><a class="@social.Name text-white-2" href="@social.Link" target="_blank"><i class="fab @social.CssClass"></i></a></li>
            }
        </ul>
    </div>
}

