@using Store.Application.Services.SiteContacts.Queries.GetSocialMediaForSite;
@model List<GetSocialMediaSiteDto>
@if(Model.Count>0)
{
    <div class="socialmedia elements-social social-icon-style-7">
        @foreach (var social in Model)
        {
            <a class="@social.Name" href="@social.Link" target="_blank"><i class="fab @social.CssClass"></i><span></span></a>
        }
    </div>
}
