@using Store.Application.Services.SiteContacts.Queries.GetSocialMediaForSite;
@model List<GetSocialMediaSiteDto>
@if (Model.Count > 0)
{
    <div class="header-social-icon d-none d-md-inline-block">
        @foreach (var social in Model)
        {
            <a href="@social.Link" title="@social.Name" target="_blank"><i class="fab @social.CssClass" aria-hidden="true"></i></a>

        }
    </div>
}
